{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
    "plugin:react/recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "import/prefer-default-export": "on",
    "no-unused-expressions": "on"
  },
  "overrides": [
    {
      "files": ["**/*"], // Or *.test.js
      "rules": {
        "require-jsdoc": "off"
      }
    }
  ]
}
